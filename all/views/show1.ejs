 <% console.dir(item) %>
 <div id="moviebox" >
   <div class="row" style="border:4px solid black">
<div class="col-md-4">
        <img src="https://image.tmdb.org/t/p/w500<%= item.poster_path %>" alt="" >
      

</div>
<div class="col-md-8">

     <div id="mydiv">  <img src="https://image.tmdb.org/t/p/w500<%= item.backdrop_path %>" style="position: relative;">


</div>
       
 

    
</div>

   </div>

</div>

<div class="pp">
<div class="trailer-box" >


      <iframe width="100%" height="400" src="https://www.youtube.com/embed/<%= trailer.videos.results[0].key %>?rel=0&amp;start=5" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
     


</div>
     
</div>


<div>
       <% reviews.results.forEach(function(review) { %>
            <div class="col-sm-6 col-md-4 col-lg-4">
            <h2><%= review.author %></h2>
            <p><%= review.content %></p>
            </div>
      
      <% }) %> 

    

</div>








